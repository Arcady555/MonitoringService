        <url-pattern>/spec-month</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletName25</servlet-name>
        <url-pattern>/view-data-history</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletName26</servlet-name>
        <url-pattern>/view-last-data</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletName100</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>

</web-app>
